#!/home/rtorrent/.local/pyroscope/bin/python2
# EASY-INSTALL-ENTRY-SCRIPT: 'pyrocore','console_scripts','rtmv'
__requires__ = 'pyrocore'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pyrocore', 'console_scripts', 'rtmv')()
    )
